Question Link: https://www.hackerrank.com/challenges/service-lane/problem?h_r=profile

Solution:

import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int size=sc.nextInt();
        int numberOfTestCases=sc.nextInt();
        int a,b;
        int smallest=4;
        int j=0;
        int[] arr=new int[size];
        int[] arr1=new int[size];
        for(int i=0;i<size;i++){
            arr[i]=sc.nextInt();
        }
        while(numberOfTestCases>0){
          a=sc.nextInt();
          b=sc.nextInt();
          for(int i=a;i<=b;i++){
            if(arr[i]<smallest){
                smallest = arr[i];
            }
            if(i==b){
            arr1[j]=smallest;
            j++;
            smallest=4;
            }
            
          }
          numberOfTestCases--;
        }
         for(int i=0;i<size;i++){
             if(arr1[i]!=0){
             System.out.println(arr1[i]);
             }
             else
             continue;
         }
        
    }
}





